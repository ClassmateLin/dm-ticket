accounts:
    # .damai.cn中的cookie字符串
    - cookie: ""
      remark: 账号1
      # 抢票轮训间隔, 单位毫秒
      interval: 30

      # 最早提前多少毫秒发包。 开抢剩余时间戳 = 开始售票的时间戳 - 当前时间戳。 当开抢剩余时间戳时间小于 submit_time时, 开始发送数据包。
      earliest_submit_time: 5

      # 请求时间 时间戳毫秒 用于捡漏 . 例如 50分是开票时间 但没有抢到,那么这里可以设置55分再抢一次,可能抢到二次放出的票
      # 1685415600100
      request_time: -1

      # 重试次数 
      retry_times: 2

      # 重试间隔 单位毫秒
      retry_interval: 100

      # 监控配置 可以监控多场次 多票档  按优先顺序抢票 不支持同时抢多张，同时抢可能会导致异常
      monitor:
        # 是否开启监控
        enable: false
        # 监控间隔 单位毫秒
        interval: 1000
        # 监控的场次 下面的配置就是监控 1场次的 5票档 和 3票档  2场次的 2票档和1票档  按配置顺序是否于余票进行抢票
        sessions:
          - index: 1
            grades: 5,3
          - index: 2
            grades: 2,1

      # 钉钉机器人通知
      dingtalk_notify: false
      # 钉钉机器人的token
      dingtalk_token: token

      ticket:
        # 需要抢购的门票ID, 门票详情页URL中的itemId.如:https://m.damai.cn/damai/detail/item.html?utm=&itemId=710947802955
        id: "721069251830"
        # 需要抢购的门票数量
        num: 3
        # 需要抢购的场次序号.
        sessions: 1
        # 需要抢购的票档序号
        grade: 5


